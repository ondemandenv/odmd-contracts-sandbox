    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "aws-cdk": "2.178.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "aws-cdk-lib": "2.192.0",
    "constructs": "^10.0.0",

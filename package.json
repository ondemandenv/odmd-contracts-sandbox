{
  "name": "@ondemandenv/odmd-contracts-sandbox",
  "version": "0.1.3",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "main": "index.js",
  "repository": "https://github.com/ondemandenv/odmd-contracts-sandbox.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@ondemandenv.dev/contracts-lib-base": "0.1.10"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "aws-cdk-lib": "2.203.1",
    "constructs": "^10.0.0",
    "js-yaml": "^4.1.0",
    "@types/js-yaml": "^4.0.9",
    "cross-env": "^7.0.3",
    "glob": "^11.0.0",
    "cpx": "^1.5.0"
  },
  "peerDependencies": {
    "constructs": "^10.0.0",
    "aws-cdk-lib": "2.203.1"
  },
  "scripts": {
    "prebuild": "ts-node .scripts/generate-exports.ts",
    "build": "tsc --build &&  cpx \"lib/repos/**/docs/**/*\" dist/lib/repos || true",
    "watch": "tsc --build -w",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "audit": "npm audit fix",
    "test": "npm run clean && npm run build &&  cross-env ODMD_rev_ref=b..main CDK_DEFAULT_ACCOUNT=123 CDK_DEFAULT_REGION=us-west-1 CDK_CLI_VERSION=2.164.1  jest  --verbose --detectOpenHandles --forceExit",
    "buildversion": "npm run build && npm version patch",
    "cdk-ls": "npm run build && cross-env ODMD_build_id=spring-rds-img ODMD_rev_ref=b..odmdSbxUsw1 cdk ls --profile sandbox-workspace2",
    "generate-exports": "ts-node .scripts/generate-exports.ts",
    "pack-tgz": "npm run test && npm pack",
    "pubpub": "npm run test && npm run cdk-ls && npm publish"
  }
}
